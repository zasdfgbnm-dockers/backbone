[connection]
id=gretap_{name}
uuid={uuid4}
type=ip-tunnel
interface-name=gretap_{name}
permissions=
master=bridge
slave-type=bridge

[ip-tunnel]
local={local_ip}
mode=10
remote={allowed_ips}

[ipv4]
dns-search=
method=disabled

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=disabled

[proxy]
